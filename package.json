{
    "name": "@skit/x.naive-ui",
    "title": "X Naive-UI",
    "description": "An extension to the Naive-UI component library, that enables slots and template style API for DataTable/Dropdown/Menu/Select/Tree. 一个对 Naive-UI 组件库的扩展，使 DataTable/Dropdown/Menu/Select/Tree 等组件支持插槽模板式的写法。",
    "keywords": [
        "naive-ui",
        "component library",
        "ui framework",
        "ui",
        "vue",
        "vue3"
    ],
    "version": "0.4.0",
    "author": "Fu Diwei",
    "repository": {
        "type": "git",
        "url": "https://github.com/fudiwei/x.naive-ui.git"
    },
    "license": "MIT",
    "type": "module",
    "main": "lib/index.cjs",
    "module": "es/index.mjs",
    "types": "es/index.d.ts",
    "files": [
        "dist/*",
        "lib/*",
        "es/*",
        "types/*",
        "!types/shims-*.d.ts"
    ],
    "exports": {
        ".": {
            "default": "./es/index.mjs",
            "import": "./es/index.mjs",
            "require": "./lib/index.cjs",
            "types": "./es/index.d.ts"
        }
    },
    "engines": {
        "node": ">=18.12",
        "npm": ">=8",
        "pnpm": ">=8"
    },
    "scripts": {
        "build": "rollup -c && vue-tsc -p tsconfig.dts-cjs.json && vue-tsc -p tsconfig.dts-esm.json",
        "demo": "vite --config demo/vite.config.ts"
    },
    "dependencies": {
        "naive-ui": "^2.34.4",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@babel/core": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-node-resolve": "^15.2.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-terser": "^0.4.3",
        "@rollup/plugin-typescript": "^11.1.2",
        "@rushstack/eslint-patch": "^1.3.3",
        "@types/node": "~18.16.0",
        "@vitejs/plugin-vue": "^4.3.1",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/babel-plugin-jsx": "^1.1.5",
        "@vue/compiler-sfc": "^3.3.4",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-standard": "^8.0.1",
        "@vue/eslint-config-typescript": "^11.0.3",
        "@vue/runtime-core": "^3.3.4",
        "deepmerge": "^4.3.1",
        "eslint": "^8.47.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "highlight.js": "^11.8.0",
        "prettier": "^3.0.2",
        "rollup": "^3.28.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-vue": "^6.0.0",
        "treemate": "^0.3.11",
        "typescript": "~5.1.0",
        "vite": "^4.4.9",
        "vue-router": "^4.2.4",
        "vue-tsc": "^1.8.8"
    },
    "peerDependencies": {
        "naive-ui": "^2.34.0",
        "vue": "^3.2.0"
    }
}
